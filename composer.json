{
    "name": "antoniog85/scraper",
    "license": "MIT",
    "type": "project",
    "description": "A simple scraper framework, based on goutte",
    "authors": [
        {
            "name": "Antonio Giuliana",
            "email": "antonio@antoniogiuliana.com"
        }
    ],
    "autoload": {
        "psr-4": { "Scraper\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": "7.*",
        "silex/silex": "~1.3",
        "fabpot/goutte": "^3.1",
        "doctrine/dbal": "~2.2",
        "vlucas/phpdotenv": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.0.*",
        "michelf/php-markdown": "^1.6",
        "erusev/parsedown": "^1.6",
        "phpunit/php-invoker": "^1.1"
    },
    "minimum-stability": "stable"
}
